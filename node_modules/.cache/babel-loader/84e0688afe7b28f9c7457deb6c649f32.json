{"ast":null,"code":"import { defineComponent, ref, computed, nextTick } from \"vue\";\nimport NavBar from \"../components/NavBar.vue\";\nimport FooterView from \"../components/FooterView.vue\";\nexport default defineComponent({\n  name: \"HomeView\",\n\n  setup() {\n    const mainImage = ref(\"\");\n    const aboutImage = ref(\"Sofiane.png\");\n    const filterCategory = ref(\"all\");\n    const items = ref([{\n      img: \"Twitter_1.png\",\n      title: \"My_Twitter\",\n      description: \"Clone de Twitter\",\n      category: \"fullstack\"\n    }, {\n      img: \"FreeAds.png\",\n      title: \"FreeAds\",\n      description: \"Plateforme d'annonces\",\n      category: \"fullstack\"\n    }, {\n      img: \"Goodrenov.png\",\n      title: \"Good Renov\",\n      description: \"Entreprise de rénovation\",\n      category: \"frontend\"\n    }, {\n      img: \"Spotify.png\",\n      title: \"Spotifine\",\n      description: \"Application musicale\",\n      category: \"frontend\"\n    }, {\n      img: \"cinema.png\",\n      title: \"My_Cinema\",\n      description: \"Gestion de cinéma\",\n      category: \"backend\"\n    }]);\n    const filteredItems = computed(() => {\n      return filterCategory.value === \"all\" ? items.value : items.value.filter(item => item.category === filterCategory.value);\n    });\n\n    async function loadImages() {\n      await nextTick();\n      mainImage.value = \"your_main_image.png\";\n    }\n\n    loadImages();\n    return {\n      mainImage,\n      aboutImage,\n      filterCategory,\n      filteredItems\n    };\n  },\n\n  components: {\n    NavBar,\n    FooterView\n  }\n});","map":{"version":3,"mappings":"AAgEA,SAASA,eAAT,EAA0BC,GAA1B,EAA+BC,QAA/B,EAAyCC,QAAzC,QAAyD,KAAzD;AACA,OAAOC,MAAP,MAAmB,0BAAnB;AACA,OAAOC,UAAP,MAAuB,8BAAvB;AAEA,eAAeL,eAAe,CAAC;EAC7BM,IAAI,EAAE,UADuB;;EAE7BC,KAAK,GAAG;IACN,MAAMC,SAAQ,GAAIP,GAAG,CAAC,EAAD,CAArB;IACA,MAAMQ,UAAS,GAAIR,GAAG,CAAC,aAAD,CAAtB;IACA,MAAMS,cAAa,GAAIT,GAAG,CAAC,KAAD,CAA1B;IACA,MAAMU,KAAI,GAAIV,GA